  "scripts": {
    "hot": "babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.hot.js && babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack/webpack.hot.js",
    "lint": "eslint .",
    "webpack-server": "babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.server.babel.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build/ --exec \"node build/bundle.js\"",
    "dev:build-server": "babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.server.dev.js --watch",
    "dev:build-client": "rm -rf ./public && babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.client.dev.js --watch",
    "build" : "NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.client.prod.js --production && babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.server.prod.js --production",
    "production": "node build/bundle.js"
  },
  "engines": {
    "node": "8.15.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@material-ui/core": "3.9.1",
